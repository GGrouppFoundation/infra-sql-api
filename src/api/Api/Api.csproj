    <PackageReference Include="GGroupp.Infra.Sql.Api.Provider.Abstractions" Version="0.1.1" />
    <PackageReference Include="PrimeFuncPack.Dependency.Core" Version="2.0.2" />
  </ItemGroup>

</Project>